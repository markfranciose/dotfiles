[user]
name = Mark Franciose
email = mark.franciose@gmail.com

[alias]
	cb		= checkout -b
	co		= checkout
	st		= status
	exp		= archive -o latest-repo.tar.gz -9 --prefix=latest/
	# print a pretty version of the log 
	plog	= !git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	
[color]
	ui = true

[color "branch"]
	current = green reverse
	local = blue
	remote = green
